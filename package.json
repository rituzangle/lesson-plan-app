{
  "name": "lesson-plan-app",
  "version": "1.0.0",
  "description": "Inclusive K-12 lesson planning app with accessibility-first design",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo build",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:web": "expo build:web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "type-check": "tsc --noEmit",
    "setup": "bash scripts/setup.sh",
    "code-gen": "python ./scripts/code-gen.py",
    "git-sync": "bash ./scripts/git-sync.sh",
    "deploy": "bash ./scripts/deploy.sh",
    "setup:database": "npm install @react-native-async-storage/async-storage crypto-js && npm install --save-dev @types/crypto-js",
    "setup:onboarding": "npm install @react-native-async-storage/async-storage crypto-js && npm install --save-dev @types/crypto-js",
    "test:database": "echo \"Database tests integrated in app\"",
    "clean:database": "echo \"Use clearDatabaseForTesting() function\"",
    "build:docs": "echo \"Documentation auto-generated\"",
    "docs:install": "cd website && npm install",
    "docs:dev": "cd website && npm start",
    "docs:build": "cd website && npm run build",
    "docs:deploy": "cd website && npm run deploy",
    "docs:serve": "cd website && npm run serve",
    "build:assessment": "expo build --platform all",
    "test:assessment": "jest --testPathPattern=assessment",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:security": "npm audit && echo \"Security audit completed\"",
    "deploy:staging": "expo publish --release-channel staging",
    "setup:daily": "node scripts/daily-setup.js"
  },
  "keywords": [
    "education",
    "lesson-planning",
    "accessibility",
    "inclusive",
    "k12",
    "react-native",
    "expo"
  ],
  "author": "rituzangle <rsangha@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/drawer": "^7.5.3",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "expo": "~53.0.19",
    "expo-accessibility": "^0.1.1",
    "expo-av": "~15.1.7",
    "expo-camera": "~16.1.10",
    "expo-constants": "~17.1.7",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-print": "~14.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-sharing": "~13.1.5",
    "expo-speech": "~13.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "lodash": "^4.17.21",
    "react": "19.1.0",
    "react-hook-form": "^7.60.0",
    "react-native": "0.80.1",
    "react-native-barcode-builder": "^2.0.2",
    "react-native-calendars": "^1.1313.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-document-picker": "^9.3.1",
    "react-native-elements": "^3.4.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "~2.27.1",
    "react-native-html-to-pdf": "^0.12.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^13.0.2",
    "react-native-paper": "^5.14.5",
    "react-native-pdf": "^6.7.7",
    "react-native-progress": "^5.0.1",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.18.0",
    "react-native-safe-area-context": "5.5.1",
    "react-native-screens": "~4.11.1",
    "react-native-sound": "^0.11.2",
    "react-native-super-grid": "^6.0.1",
    "react-native-svg": "15.12.0",
    "react-native-tts": "^4.1.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-voice": "^3.2.4",
    "uuid": "^11.1.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.20",
    "@types/react": "~19.1.8",
    "@types/react-native": "~0.73.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "eslint-config-expo": "^9.2.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-expo": "~53.0.9",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/types/**/*"
    ]
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rituzangle/lesson-plan-app.git"
  },
  "bugs": {
    "url": "https://github.com/rituzangle/lesson-plan-app/issues",
    "email": "rsangha@gmail.com"
  },
  "homepage": "https://github.com/rituzangle/lesson-plan-app#readme"
}